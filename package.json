{
  "name": "webapp-schulverwaltung",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:browserstack": "ng serve --host 0.0.0.0 --disable-host-check --port 8000",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test --browsers=ChromeHeadless",
    "test:brief": "ng test --browsers=ChromeHeadless --reporters brief",
    "lint": "ng lint",
    "format": "prettier --write \"./**/*.{js,ts,json,css,scss,html,md,yaml}\"",
    "format:upgrade": "npm install --save-exact prettier@latest && npm run format",
    "preanalyze": "ng build --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/$(ng config defaultProject)/stats.json",
    "postanalyze": "rm dist/$(ng config defaultProject)/stats.json",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "-": "0.0.1",
    "@angular/animations": "^13.2.1",
    "@angular/common": "^13.2.1",
    "@angular/compiler": "^13.2.1",
    "@angular/core": "^13.2.1",
    "@angular/elements": "^13.2.1",
    "@angular/forms": "^13.2.1",
    "@angular/localize": "^13.2.1",
    "@angular/platform-browser": "^13.2.1",
    "@angular/platform-browser-dynamic": "^13.2.1",
    "@angular/router": "^13.2.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ng-select/ng-select": "^8.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^4.6.0",
    "core-js": "~3.21.0",
    "date-fns": "^2.28.0",
    "document-register-element": "^1.14.10",
    "fp-ts": "^2.11.8",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "lodash-es": "^4.17.21",
    "monocle-ts": "^2.3.12",
    "newtype-ts": "^0.3.5",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.5.3",
    "tslib": "^2.3.1",
    "watchpack": "^2.3.1",
    "xss": "^1.0.10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fibers": "^5.0.1",
    "html-validate": "^6.3.1",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.15",
    "karma-brief-reporter": "^0.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "^0.0.33",
    "lint-staged": "~12.3.3",
    "prettier": "2.2.1",
    "ts-node": "~10.5.0",
    "typescript": "~4.5.5"
  },
  "lint-staged": {
    "./src/app/**/*.{ts,html}": [
      "eslint"
    ],
    "./src/app/**/*.ts": [
      "prettier --write"
    ],
    "./**/*.{js,json,css,scss,html,md,yaml}": [
      "prettier --write"
    ],
    "./src/app/**/*.html": [
      "html-validate"
    ]
  }
}
